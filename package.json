{
  "name": "meteor-template",
  "private": true,
  "scripts": {
    "start": "meteor run --exclude-archs=web.browser.legacy",
    "update": "npx concurrently@latest --prefix-colors auto --names meteor,node 'npm run update:meteor' 'npm run update:node'",
    "update:meteor": "meteor update --all-packages",
    "update:node": "npx npm-check-updates@latest --upgrade && npm update",
    "check": "np tsc --noEmit --skipLibCheck",
    "format": "npx prettier@latest --write . --ignore-path .gitignore",
    "storybook": "storybook dev -p 6006",
    "storybook-update": "npx storybook@next upgrade --prerelease",
    "test": "test-storybook"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.1",
    "bootstrap": "^5.3.2",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.4.6",
    "@storybook/addon-interactions": "^7.4.6",
    "@storybook/addon-links": "^7.4.6",
    "@storybook/blocks": "^7.4.6",
    "@storybook/jest": "^0.2.3",
    "@storybook/react": "^7.4.6",
    "@storybook/react-vite": "^7.4.6",
    "@storybook/test-runner": "^0.13.0",
    "@storybook/testing-library": "^0.2.2",
    "@types/meteor": "^2.9.4",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.10",
    "storybook": "^7.4.6",
    "typescript": "^5.2.2",
    "vite": "^4.4.10"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.tsx",
      "server": "server/main.ts"
    }
  },
  "prettier": {
    "printWidth": 150,
    "singleAttributePerLine": true
  }
}
