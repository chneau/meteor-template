{
  "name": "meteor-template",
  "private": true,
  "scripts": {
    "start": "meteor run --exclude-archs=web.browser.legacy",
    "update": "npm exec --yes concurrently@latest -- --prefix-colors auto --names meteor,node 'npm run update:meteor' 'npm run update:node'",
    "update:meteor": "meteor update --all-packages",
    "update:node": "npm exec --yes npm-check-updates@latest -- --upgrade && npm update",
    "check": "npm exec tsc -- --noEmit --skipLibCheck",
    "format": "npm exec --yes prettier@latest -- --write . --ignore-path .gitignore",
    "storybook": "storybook dev -p 6006",
    "test": "test-storybook"
  },
  "dependencies": {
    "@babel/runtime": "^7.21.0",
    "bootstrap": "^5.2.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.2",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.0-beta.62",
    "@storybook/addon-interactions": "^7.0.0-beta.62",
    "@storybook/addon-links": "^7.0.0-beta.62",
    "@storybook/blocks": "^7.0.0-beta.62",
    "@storybook/react": "^7.0.0-beta.62",
    "@storybook/react-vite": "^7.0.0-beta.62",
    "@storybook/test-runner": "^0.10.0-next.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/meteor": "^2.9.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "storybook": "^7.0.0-beta.62",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.tsx",
      "server": "server/main.ts"
    }
  },
  "prettier": {
    "printWidth": 150,
    "singleAttributePerLine": true
  }
}
